{
  "partitionUuid": "938e16fe-a216-4fe1-92bd-dcab49646fb9",
  "pipelines": [
    {
      "name": "tester-pipeline",
      "checkpoint": {
        "outputConnector": "/tmp/checkpoints",
        "externalType": "ExternalLocalFile"
      },
      "enableTracing": true,
      "enableMetrics": true,
      "processes": [
        {
          "name": "annotate-cicd",
          "instrumentation": {
            "enableTracing": true,
            "latency": true,
            "counter": true
          }
        },
        {
          "name": "aggregate-author",
          "instrumentation": {
            "enableTracing": true
          },
          "retryStrategy": {
            "numRetries": 5,
            "initialBackOffMs": 100
          }
        },
        {
          "name": "count-repo",
          "instrumentation": {
            "enableTracing": true
          },
          "retryStrategy": {
            "numRetries": 5,
            "initialBackOffMs": 100
          }
        },
        {
          "name": "completion-hashjoin",
          "instrumentation": {
            "enableTracing": true
          }
        },
        {
          "name": "tee-out-to-file",
          "instrumentation": {
            "enableTracing": true
          }
        },
        {
          "name": "tee-git-events",
          "instrumentation": {
            "enableTracing": true
          }
        },
        {
          "name": "tee-cicd-completion",
          "instrumentation": {
            "enableTracing": true
          }
        }
      ]
    }
  ],
  "processDefinitions": [
    {
      "annotator": {
        "name": "annotate-cicd",
        "annotations": [
          {
            "fieldKey": "version-control",
            "value": "git-dev",
            "condition": {
              "exists": {
                "ops": [
                  {
                    "key": "pusher.name",
                    "op": "Exists"
                  },
                  {
                    "key": "before",
                    "op": "Exists"
                  },
                  {
                    "key": "after",
                    "op": "Exists"
                  },
                  {
                    "key": "vcs_revision",
                    "op": "NotExists"
                  }
                ]
              }
            }
          },
          {
            "fieldKey": "deploy",
            "value": "circleci-dev",
            "condition": {
              "exists": {
                "ops": [
                  {
                    "key": "pusher.name",
                    "op": "NotExists"
                  },
                  {
                    "key": "vcs_revision",
                    "op": "Exists"
                  }
                ]
              }
            }
          }
        ]
      }
    },
    {
      "tee": {
        "name": "tee-out-to-file",
        "outputConnector": "/tmp/cicd-events",
        "externalType": "ExternalLocalFile",
        "transformerSpecs": [
          {
            "transformation": {
              "transformationType": "TransformCopy"
            }
          }
        ]
      }
    },
    {
      "aggregator": {
        "name": "count-repo",
        "stateStore": "memKVStore",
        "condition": {
          "exists": {
            "ops": [
              {
                "key": "repository.name",
                "op": "Exists"
              }
            ]
          }
        },
        "aggregation": {
          "key": "repository.name",
          "aggregationType": "AggCount",
          "groupByKeys": ["repository.name"]
        }
      }
    },
    {
      "aggregator": {
        "name": "aggregate-author",
        "stateStore": "memKVStore",
        "condition": {
          "expression": {
            "comparator": {
              "lhs": {
                "variable": {
                  "name": "version-control"
                }
              },
              "rhs": {
                "literal": "git-dev"
              },
              "op": "Equal"
            }
          }
        },
        "aggregation": {
          "key": "pusher.name",
          "aggregationType": "AggDiscreteHistogram",
          "groupByKeys": []
        }
      }
    },
    {
      "completer": {
        "name": "completion-hashjoin",
        "stateStore": "memKVStore",
        "completion": {
          "joinKeys": [
            "vcs_revision",
            "after"
          ],
          "timeoutMs": 3600000
        }
      }
    },
    {
      "filter": {
        "name": "filter-internal",
        "regex": "^agglo.*",
        "keepMatched": false
      }
    },
    {
      "tee": {
        "name": "tee-git-events",
        "condition": {
          "expression": {
            "comparator": {
              "lhs": {
                "variable": {
                  "name": "version-control"
                }
              },
              "rhs": {
                "literal": "git-dev"
              },
              "op": "Equal"
            }
          }
        },
        "outputConnector": "/tmp/git-events",
        "externalType": "ExternalLocalFile",
        "transformerSpecs": [
          {
            "transformation": {
              "transformationType": "TransformCopy"
            }
          }
        ]
      }
    },
    {
      "tee": {
        "name": "tee-cicd-completion",
        "condition": {
          "expression": {
            "comparator": {
              "lhs": {
                "variable": {
                  "name": "internal:completion:completion-hashjoin"
                }
              },
              "rhs": {
                "literal": "complete"
              },
              "op": "Equal"
            }
          }
        },
        "outputConnector": "/tmp/cicd-completion",
        "externalType": "ExternalLocalFile",
        "transformerSpecs": [
          {
            "transformation": {
              "transformationType": "TransformCopy"
            }
          }
        ]
      }
    }
  ],
  "externalSystems": [
    {
      "externalType": "ExternalKVStore",
      "name": "memKVStore",
      "connectionString": "local"
    }
  ]
}