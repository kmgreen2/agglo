{
  "partitionUuid": "938e16fe-a216-4fe1-92bd-dcab49646fb9",
  "pipelines": [
    {
      "name": "tester-pipeline",
      "processes": [
        {
          "name": "annotate-cicd"
        },
        {
          "name": "aggregate-author"
        },
        {
          "name": "completion-hashjoin"
        },
        {
          "name": "tee-git-transform"
        },
        {
          "name": "filter-internal"
        },
        {
          "name": "spawner-print-githash"
        }
      ]
    }
  ],
  "processDefinitions": [
    {
      "annotator": {
        "name": "annotate-cicd",
        "annotations": [
          {
            "fieldKey": "version-control",
            "value": "git-dev",
            "condition": {
              "exists": {
                "ops": [
                  {
                    "key": "author",
                    "op": "Exists"
                  },
                  {
                    "key": "hash",
                    "op": "Exists"
                  },
                  {
                    "key": "githash",
                    "op": "NotExists"
                  }
                ]
              }
            }
          },
          {
            "fieldKey": "deploy",
            "value": "circleci-dev",
            "condition": {
              "exists": {
                "ops": [
                  {
                    "key": "author",
                    "op": "NotExists"
                  },
                  {
                    "key": "hash",
                    "op": "NotExists"
                  },
                  {
                    "key": "githash",
                    "op": "Exists"
                  }
                ]
              }
            }
          }
        ]
      }
    },
    {
      "aggregator": {
        "name": "aggregate-author",
        "stateStore": "memKVStore",
        "asyncCheckpoint": false,
        "forwardState": true,
        "condition": {
          "expression": {
            "comparator": {
              "lhs": {
                "variable": {
                  "name": "version-control"
                }
              },
              "rhs": {
                "literal": "git-dev"
              },
              "op": "Equal"
            }
          }
        },
        "aggregation": {
          "key": "author",
          "aggregationType": "AggDiscreteHistogram",
          "groupByKeys": []
        }
      }
    },
    {
      "completer": {
        "name": "completion-hashjoin",
        "stateStore": "memKVStore",
        "completion": {
          "joinKeys": [
            "hash",
            "githash"
          ],
          "timeoutMs": 3600000
        }
      }
    },
    {
      "filter": {
        "name": "filter-internal",
        "regex": "^agglo.*",
        "keepMatched": false
      }
    },
    {
      "spawner": {
        "name": "spawner-print-githash",
        "delayInMs": 0,
        "doSync": true,
        "condition": {
          "exists": {
            "ops": [
              {
                "key": "githash",
                "op": "Exists"
              }
            ]
          }
        },
        "job": {
          "runnable": {
            "pathToExec": "../../../bin/printvals",
            "cmdArgs": [
              "/tmp/testPipelinesBasic",
              "githash"
            ]
          }
        }
      }
    },
    {
      "transformer": {
        "name": "git-transform",
        "specs": [
          {
            "sourceField": "author",
            "targetField": "gitAuthor",
            "transformation": {
              "transformationType": "TransformCopy"
            }
          },
          {
            "sourceField": "hash",
            "targetField": "gitHash",
            "transformation": {
              "transformationType": "TransformCopy"
            }
          }
        ]
      }
    },
    {
      "tee": {
        "name": "tee-git-transform",
        "condition": {
          "expression": {
            "comparator": {
              "lhs": {
                "variable": {
                  "name": "version-control"
                }
              },
              "rhs": {
                "literal": "git-dev"
              },
              "op": "Equal"
            }
          }
        },
        "outputConnectorRef": "memKVStore",
        "transformerRef": "git-transform"
      }
    }
  ],
  "externalSystems": [
    {
      "externalType": "ExternalKVStore",
      "name": "memKVStore",
      "connectionString": "local"
    }
  ]
}